<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical">
	<ImageView android:id="@+id/imageView1"
		android:layout_height="wrap_content" android:src="@drawable/logo01_3"
		android:layout_width="match_parent" />
	<RelativeLayout android:id="@+id/relativeLayout1"
		android:layout_width="fill_parent" android:layout_height="60dp">
		<TextView android:layout_height="wrap_content" android:id="@+id/textView1"
			android:layout_width="wrap_content" android:text="검색결과 "
			android:textColor="#ffffff" android:textStyle="bold"
			android:layout_alignParentLeft="true" android:textSize="30dp"
			android:layout_centerVertical="true" />
		<Button android:layout_height="wrap_content" android:text="다시 검색 "
			android:id="@+id/btnclose" android:layout_width="wrap_content"
			android:layout_alignParentRight="true" android:layout_centerVertical="true" />
	</RelativeLayout>
	<ListView android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/mylist" />
</LinearLayout>
