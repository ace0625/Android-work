<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:background="#ffffff"
	android:layout_height="90dp">
	<ImageView android:id="@+id/img" android:src="@drawable/noimage"
		android:layout_alignParentLeft="true" android:layout_centerVertical="true"
		android:padding="0dp" android:layout_height="70dp"
		android:layout_width="70dp" />
	<TextView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:textColor="#000000"
		android:layout_toRightOf="@+id/img" android:text="마이네임이즈 칸 "
		android:id="@+id/txttitle" android:textSize="20dp" />
	<TextView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:textColor="#000000"
		android:id="@+id/txtSub" android:layout_below="@+id/txttitle"
		android:text="세부 설명 " android:layout_toRightOf="@+id/img"
		android:textSize="16dp" />
	<TextView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_alignParentRight="true"
		android:layout_centerVertical="true" android:textColor="#000000"
		android:textSize="25dp" android:layout_marginRight="5dp" android:text="10.0 "
		android:id="@+id/txtgrade" />
</RelativeLayout>
