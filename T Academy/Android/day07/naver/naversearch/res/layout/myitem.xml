<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:background="#ffffff" android:layout_height="60dp">
    <TextView android:layout_alignParentLeft="true" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/txtnum" android:text="01" android:textSize="25dp" android:layout_centerVertical="true" android:textColor="#000000"></TextView>
    <TextView android:id="@+id/textView1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_toRightOf="@+id/txtnum" android:layout_centerVertical="true" android:text="안드로이드 " android:textSize="30dp" android:textColor="#000000" android:layout_marginLeft="5dp"></TextView>
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_alignParentRight="true" android:text="209" android:layout_centerVertical="true" android:textColor="#000000" android:textSize="25dp" android:id="@+id/txtcnt" android:layout_marginRight="5dp"></TextView>
    <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/img" android:layout_centerVertical="true" android:layout_toLeftOf="@+id/txtcnt" android:src="@drawable/up"></ImageView>
</RelativeLayout>
